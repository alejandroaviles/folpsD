Cell[BoxData[
 RowBox[{
  RowBox[{"integrand", "[", 
   RowBox[{"l1_", ",", "l2_", ",", "L_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Nl1l2L", "[", 
    RowBox[{"l1", ",", "l2", ",", "L"}], "]"}], 
   RowBox[{"Hl1l2L", "[", 
    RowBox[{"l1", ",", "l2", ",", "L"}], "]"}], 
   RowBox[{
    RowBox[{
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Conjugate", "[", 
         RowBox[{"ylm", "[", 
          RowBox[{"l2", ",", 
           RowBox[{"-", "M"}], ",", 
           RowBox[{"-", "tht12"}], ",", "0"}], "]"}], "]"}], 
        RowBox[{"Conjugate", "[", 
         RowBox[{"ylm", "[", 
          RowBox[{"L", ",", "M", ",", "w", ",", "phi"}], "]"}], "]"}]}], ",", 
       
       RowBox[{"{", 
        RowBox[{"M", ",", 
         RowBox[{"-", "L"}], ",", "L"}], "}"}]}], "]"}], "/", 
     RowBox[{"(", 
      RowBox[{"4", "Pi"}], ")"}]}], "/", "2"}]}]}]], "Input",
