Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ylm", "[", 
    RowBox[{"L_", ",", "M_", ",", "tht_", ",", "phi_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Sqrt", "[", 
     FractionBox[
      RowBox[{"4", " ", "Pi"}], 
      RowBox[{
       RowBox[{"2", "L"}], " ", "+", "1"}]], "]"}], 
    RowBox[{"SphericalHarmonicY", "[", 
     RowBox[{"L", ",", "M", ",", "tht", ",", "phi"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Hl1l2L", "[", 
    RowBox[{"l1_", ",", "l2_", ",", "L_"}], "]"}], ":=", 
   RowBox[{"ThreeJSymbol", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"l1", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"l2", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"L", ",", "0"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Nl1l2L", "[", 
    RowBox[{"l1_", ",", "l2_", ",", "L_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "l1"}], "+", "1"}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "l2"}], "+", "1"}], ")"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "L"}], "+", "1"}], ")"}]}]}], ";"}]}], "Input",
