        RowBox[{"n_.", " ", "tht12"}], "]"}], ":>", 
       RowBox[{"ChebyshevT", "[", 
        RowBox[{"n", ",", "x"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"n_.", " ", "tht12"}], "]"}], ":>", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{"1", "-", 
          RowBox[{"x", "^", "2"}]}], "]"}], " ", 
        RowBox[{"ChebyshevU", "[", 
         RowBox[{
          RowBox[{"n", "-", "1"}], ",", "x"}], "]"}]}]}]}], "}"}]}], "//", 
   "Expand"}]}]}], "Input",
