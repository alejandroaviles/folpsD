Cell[BoxData[{
 RowBox[{
  RowBox[{"ToMu", "[", "expr_", "]"}], ":=", 
  RowBox[{
   RowBox[{"expr", "/.", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Cos", "[", 
        RowBox[{"n_.", " ", "w"}], "]"}], ":>", 
       RowBox[{"ChebyshevT", "[", 
        RowBox[{"n", ",", "mu"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"n_.", " ", "w"}], "]"}], ":>", 
